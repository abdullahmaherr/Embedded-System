#include<stdio.h>

#define ARR_CREW_ID_SIZE 100000
#define ARR_JOB_ID_SIZE 100000

int main(void)
{
	int i;
	int crew_id[ARR_CREW_ID_SIZE];
	int job_id[ARR_JOB_ID_SIZE];

	int crew_id_count;
	scanf("%d", &crew_id_count);

	for(i = 0; i < crew_id_count; i++)
	{
		scanf("%d", &crew_id[i]);
	}

	int job_id_count;
	scanf("%d", &job_id_count);
}
